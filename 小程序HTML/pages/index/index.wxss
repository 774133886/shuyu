/* pages/index/index.wxss */
page{
  background-color: #fef9f8;
  box-sizing: border-box;
  height: 100%;
  /* padding: 0 25rpx; */
  -webkit-box-shadow: 0 1px 5px 0 #f8eeec inset;
  box-shadow: 0 1px 5px 0 #f8eeec inset;
}
.indexCen{
  padding: 0 25rpx;
  box-sizing: border-box;
}
.hgt_100{
  height: 100%;
}
.indexHead{
  padding: 30rpx 0;
  margin-bottom: 10rpx;
}
.ih_box{
  height: 280rpx;
  padding: 30rpx;
  border-radius: 10rpx;
  -webkit-box-shadow: 0 0 16rpx 2rpx rgba(241,124,30,.53);
  box-shadow: 0 0 16rpx 2rpx rgba(241,124,30,.53);
  z-index: 11;
}
.if_text{
  color: #ff5a71;
  text-align: center;
}
.indexFoot{
  margin-top: 45rpx;
}
.if_dot view{
  background-color: #fee3e5;
  height: 12rpx;
  border-radius: 12rpx;
  margin-right: 15rpx;
}
.if_dot view.active{
  background-color: #FF5A71;
}
.if_dot view:last-child{
  margin-right: 0;
}
.indexCtt{
  background-color: #fff;
  border-radius: 10rpx;
}
.indexCtt::-webkit-scrollbar-thumb{
  width: 6rpx;
  border-radius: 6rpx;
  background-color: #e5e5e5;
}
.icItem{
  padding: 0 40rpx 0 28rpx;
  position: relative;
}
.icItem:after{
  content: '';
  position: absolute;
  bottom: 0;
  left: 13rpx;
  width: calc(100% - 47rpx);
  border-bottom: 1px solid #e5e5e5;
}
.ic_text{
  padding: 33rpx 50rpx 33rpx 0;
  width: calc(100% - 180rpx);
}
.ic_btn{
  width: 180rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fff;
}
.ic_btn text{
  color: #fff;
  font-size: 28rpx;
}
.color-ff5a71{
  color: #ff5a71;
}
.color-f9ab71{
  color: #f9ab71;
}
.ic_btn2{
  width: 180rpx;
  font-size: 24rpx;
  text-align: right;
  color: #ff5a71;
}
.indexFixed{
  position: fixed;
  top: 250rpx;
  right: 0;
  font-size: 24rpx;
  color: #97430c;
  text-align: center;
  width: 120rpx;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #ffcd85;
  border-top-left-radius: 40rpx;
  border-bottom-left-radius: 40rpx;
  -webkit-box-shadow: 0 1px 6rpx 4rpx rgba(254,98,114,.65);
  box-shadow: 0 1px 6rpx 4rpx rgba(254,98,114,.65);
  z-index: 11;
}
.indexMask{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .79);
  z-index: 999;
}
.maskCenter{
  position: absolute;
  bottom: 0;
  left: 60rpx;
  right: 60rpx;
  height: calc(100% - 50rpx)
}
.maskCtt{
  border-radius: 10rpx;
  background-color: #fff;
  overflow: hidden;
}
.im_head{
  height: 125rpx;
  line-height: 125rpx;
  text-align: center;
}
.im_foot{
  padding: 25rpx 0;
}
.im_foot image{
  width: 72rpx;
  height: 72rpx;
  display: block;
  margin: auto;
}
.im_ctt{
  margin: 25rpx 30rpx 35rpx 30rpx;
  border: 1px solid #dcdcdc;
  width: calc(100% - 60rpx);
  box-sizing: border-box;
  overflow-y: auto;
}
.im_text{
  padding: 0 50rpx;
}
.im_text_tit{
  font-size: 0;
  text-align: center;
  margin-bottom: 15rpx;
}
.im_text_tit image{
  width: 29rpx;
  height: 33rpx;
  display: inline-block;
  vertical-align: middle;
  margin-bottom: -3rpx;
  margin-right: 10rpx;
}
.im_text_tit view{
  display: inline-block;
  vertical-align: middle;
}
.im_text .font24{
  line-height: 36rpx;
}
.im_btn{
  padding: 25rpx 0 30rpx;
}
.im_btn button{
  width: 240rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 70rpx;
  font-size: 30rpx;
  color: #fff;
  margin: auto;
}
.mask2 .maskCenter{
  width: 540rpx;
  height: 790rpx;
  bottom: 50%;
  left: 50%;
  right: none;
  transform: translate(-50%,50%);
}
.mask2 .maskCenter .maskCtt{
  box-sizing: border-box;
  padding: 50rpx 55rpx 65rpx;
  text-align: center;
}
.choiceType{
  margin-top: 50rpx;
}
.choiceType .flex_1{
  height: 280rpx;
  background-color: #fae8e8;
  border-radius: 10rpx;
  padding-top: 65rpx;
}
.icon-wechat{
  width: 91rpx;
  height: 80rpx;
}
.icon-car{
  width: 65rpx;
  height: 80rpx;
}
.choiceType .flex_1 view{
  font-size: 30rpx;
  color: #cc9a9a;
  line-height: 40rpx;
}
.choiceType .curBg4 view{
  color: #fff;
}
.im_btn2{
  width: 400rpx;
  height: 90rpx;
  font-size: 36rpx;
  line-height: 90rpx;
  border-radius: 90rpx;
  color: #fff !important;
  margin: 65rpx auto 0;
}
.shareMask image{
  margin-right: 70px;
  width: 475rpx;
  height: 164rpx;
}
.share_btn{
  width: 290rpx;
  height: 90rpx;
  font-size: 36rpx !important;
  line-height: 90rpx;
  border-radius: 90rpx;
  color: #fff !important;
  margin: 110rpx auto 0;
}
.maskBox{
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
  background-color: #fff;
  border-radius: 10rpx;
  padding: 45rpx 90rpx;
  width: 470rpx;
}
.maskBox button{
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 70rpx;
  color: #fff !important;
  font-size: 30rpx;
  margin-top: 30rpx;
}