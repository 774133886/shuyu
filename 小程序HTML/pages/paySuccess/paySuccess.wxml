<!--pages/paySuccess/paySuccess.wxml-->
<view class='Con font30'>
  <view class='weekclassCon text-center'>
    <image src='../../files/icon_right_s.png' class='paySucPic'></image>
    <view class='psText'>支付成功</view>
    <view class='wcText'>
      中版国教教育技术有限公司
    </view>
    <view class='wcText font48 m-t-30'>
      ¥{{money}}
    </view>
  </view>
  <view class='wid100 text-center'>
    <button bindtap='doneBtn' class='keepBtn'>添加助教微信</button>
  </view>
</view>
<view class='mask'>
  <view class='maskCenter payMask'>
    <view class='payMaskHead curBg7'>--请添加助教微信  开启学习之旅--</view>
    <view class='payTitle'>
      <view>扫描添加老师微信</view>
      <view>即可获得专属福利</view>
    </view>
    <view class='paysanjiao'></view>
    <image class='payCode' src='image'></image>
    <view class='payText'>微信号：{{wxname}}</view>
    <button class='curBg4' bindtap='cloneText'>复制微信号</button>
  </view>
</view>

